{
  "name": "bytic/view",
  "description": "Php Framework",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "homepage": "https://github.com/ByTIC/view",
  "require": {
    "php": "^7.0 || ^8.0",
    "bytic/collections": "^1.0 || ^2.0",
    "bytic/container": "^0.9 || ^1.0 || ^2.0",
    "bytic/helpers": "^0.9 || ^1.0 || ^2.0",
    "bytic/package-base": "^1.0",
    "bytic/pipeline": "^1.0",
    "league/plates": "^3.5"
  },
  "require-dev": {
    "bytic/http": "^0.9 || ^1.0",
    "bytic/phpqatools": "^1.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Nip\\View\\": "src",
      "Nip\\View\\Legacy\\": "legacy"
    },
    "classmap": [
      "legacy",
      "src"
    ],
    "files": []
  },
  "autoload-dev": {
    "psr-4": {
      "Nip\\View\\Tests\\": "tests/src",
      "Nip\\View\\Tests\\Fixtures\\App\\": "tests/fixtures/app"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "ergebnis/composer-normalize": true,
      "phpstan/extension-installer": true,
      "bytic/phpqatools": true
    }
  }
}
